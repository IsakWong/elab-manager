<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="elab.database.mappers.SelectExistTable">
    <select id="checkTableExistsWithSchema" resultType="java.lang.Integer">
        select count(1) from TABLES where table_schema = #{tableSchema} AND table_name = #{tableName}
    </select>
    <select id="checkTableExistsWithShow" resultType="java.util.Map">
        show tables like #{tableName}
    </select>
</mapper>