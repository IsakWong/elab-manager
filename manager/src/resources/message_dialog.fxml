<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="350.0" stylesheets="@css/controls_style.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="elab.business.message_dialog.MessageDialogController">
   <children>
      <AnchorPane fx:id="topBar" prefHeight="49.0" prefWidth="350.0" style="-fx-background-color: #3F51B5;">
         <children>
            <ImageView fx:id="minBtn" fitHeight="20.0" fitWidth="20.0" layoutX="278.0" layoutY="13.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@min.png" />
               </image>
            </ImageView>
            <ImageView fx:id="closeBtn" fitHeight="20.0" fitWidth="20.0" layoutX="315.0" layoutY="13.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@close.png" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
      <Label fx:id="label" prefHeight="85.0" prefWidth="226.0" styleClass="jfx-text-field" text="请输入正确信息！" textAlignment="CENTER" />
      <JFXButton fx:id="okBtn" prefHeight="35.0" prefWidth="92.0" text="确定" />
   </children>
</VBox>
