<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="elab.business.main_window.MainWindowController">
   <children>
       <HBox fx:id="topBar" alignment="TOP_RIGHT" prefHeight="150.0" prefWidth="200.0" style="-fx-background-color: #303F9F;">
           <children>
               <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                   <image>
                       <Image url="@min.png" />
                   </image>
                   <HBox.margin>
                       <Insets bottom="10.0" left="16.0" right="16.0" top="16.0" />
                   </HBox.margin>
               </ImageView>
               <ImageView fx:id="closeBtn" fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                   <image>
                       <Image url="@close.png" />
                   </image>
                   <HBox.margin>
                       <Insets right="16.0" top="16.0" />
                   </HBox.margin>
               </ImageView>
           </children>
       </HBox>

      <JFXTabPane fx:id="tabPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="800.0" prefWidth="1200.0" stylesheets="@css/controls_style.css" tabClosingPolicy="UNAVAILABLE" />
   </children>
</VBox>
